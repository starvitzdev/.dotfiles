cp .zshrc ~/.dotfiles/.zshrc
cp -r wallpaper ~/.dotfiles/wallpaper
cp -r .local/bin ~/.dotfiles/bin
cp -r .oh-my-posh ~/.dotfiles/.oh-my-posh
cd ~/.dotfiles

git add .
git commit -m "New settings added"
