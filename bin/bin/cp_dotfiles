cd
cp -r .config/waybar ~/.dotfiles/waybar
cp -r .config/nvim ~/.dotfiles/nvim
cp -r .config/sway ~/.dotfiles/sway
cp -r .config/fuzzel ~/.dotfiles/fuzzel
